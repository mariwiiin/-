model Lab7_2
  // time [0:0.005:0.1] 
  parameter Integer x0 = 12; // Начальное число знающих о товаре
  parameter Integer N = 1150; // максимальное число людей, которых может заинтересовать товар
  Real x(start=x0);
equation
  der(x) = (0.000076 + 0.76*x)*(N-x);
  // Пик распространения приходится на момент, когда x = 575, то есть половина от всех людей

end Lab7_2;
